{
  "name": "machine-learning",
  "main": "public/electron.js",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "concurrently \"set BROWSER=NONE && npm run react-start\" \" electron . \" ",
    "build": "npm run clean && npm run react-build && npm run electron-build && npm run copy && npm run copy-react && rimraf build",
    "copy": "cp -R ../python/{*.py,images/,output/,*.arff} ./dist/win-unpacked/resources/",
    "copy-react": "cp -R ./build/* ./dist/win-unpacked/",
    "clean": "rimraf build && rimraf dist",
    "electron-build": "electron-builder",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.3",
    "all": "0.0.0",
    "custom-electron-titlebar": "^3.2.4",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^1.2.0",
    "python-shell": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.2.0",
    "react-scripts": "^3.4.3",
    "styled-components": "^5.2.0",
    "ts-loader": "^8.0.4",
    "typescript": "~3.7.2",
    "wait-on": "^5.2.0",
    "yarn": "^1.22.5"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "rimraf": "^3.0.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "machine-learning",
    "directories": {
      "output": "dist"
    },
    "linux": {
      "target": [
        "dir"
      ]
    },
    "win": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
